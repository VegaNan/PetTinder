<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="700.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SwipeyPageController">
   <children>
      <ImageView fx:id="animalView" fitHeight="450.0" fitWidth="300.0" layoutX="75.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true" />
      <Label alignment="CENTER" layoutX="150.0" layoutY="35.0" prefHeight="40.0" prefWidth="150.0" text="Pinder" />
      <Button fx:id="profileButton" layoutX="10.0" layoutY="25.0" mnemonicParsing="false" onAction="#profilePage" prefHeight="30.0" prefWidth="80.0" text="Profile" />
      <Button fx:id="matchesButton" layoutX="340.0" layoutY="25.0" mnemonicParsing="false" onAction="#matchesPage" prefHeight="30.0" prefWidth="80.0" text="Matches" />
      <Pane layoutY="575.0" prefHeight="125.0" prefWidth="450.0">
         <children>
            <Button fx:id="yesButton" layoutX="320.0" layoutY="47.5" mnemonicParsing="false" onAction="#yesAction" prefHeight="30.0" prefWidth="70.0" text="Yes!" />
            <Button fx:id="maybeButton" layoutX="185.0" layoutY="47.5" mnemonicParsing="false" onAction="#maybeAction" prefHeight="30.0" prefWidth="80.0" text="Maybe" />
            <Button fx:id="noButton" layoutX="60.0" layoutY="47.5" mnemonicParsing="false" onAction="#noAction" prefHeight="30.0" prefWidth="60.0" text="No" />
         </children>
      </Pane>
      <Button layoutX="5.0" layoutY="350.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="40.0" text="Button" />
      <Button layoutX="405.0" layoutY="350.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="40.0" text="Button" />
   </children>
</AnchorPane>
