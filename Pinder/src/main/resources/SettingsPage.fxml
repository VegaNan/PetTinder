<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="700.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SettingsController">
   <children>
      <Pane layoutX="10.0" layoutY="10.0" prefHeight="680.0" prefWidth="430.0">
         <children>
            <Button fx:id="backButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onBackAction" prefHeight="30.0" text="Back" />
      		<Button fx:id="saveButton" layoutX="197.0" layoutY="400.0" mnemonicParsing="false" onAction="#onSaveAction" prefWidth="55.0" text="Save" />
      		<Button fx:id="switchButton" layoutX="192.0" layoutY="300.0" mnemonicParsing="false" onAction="#onSwitchAction" prefWidth="65.0" text="Switch" />
      		<Button fx:id="logoutButton" layoutX="350.0" layoutY="10.0" mnemonicParsing="false" onAction="#onLogOutAction" prefWidth="70.0" text="Logout" />
      		<Label fx:id="animalPrefLabel" layoutX="75.0" layoutY="150.0" prefHeight="100.0" prefWidth="300.0" text="Would you like to switch your animal preference?" />
         </children>
      </Pane>
   </children>
</AnchorPane>
